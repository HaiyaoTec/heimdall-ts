  "components": {
    "schemas": {
      "Game": {
        "title": "Game",
        "type": "object",
        "description": "",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "state": {
            "type": "integer",
            "description": "状态"
          },
          "typeList": {
            "type": "string",
            "description": "分类"
          },
          "tagList": {
            "type": "string",
            "description": "标签列表"
          },
          "propertySize": {
            "type": "integer",
            "description": "尺寸"
          },
          "type": {
            "type": "integer",
            "description": "游戏类型 0 普通游戏 1特殊游戏(比如活动类)"
          }
        }
      },
      "Level": {
        "title": "Level",
        "type": "object",
        "description": "",
        "properties": {
          "experienceLevel": {
            "type": "integer"
          },
          "experienceValue": {
            "type": "integer",
            "format": "int64"
          },
          "vipLevel": {
            "type": "integer"
          },
          "vipGrowthValue": {
            "type": "integer"
          },
          "crrentExperienceValue": {
            "type": "integer",
            "format": "int64",
            "description": "当前等级已达到的经验值"
          },
          "nextNeed": {
            "type": "integer",
            "format": "int64",
            "description": "到下一级需要的经验值"
          },
          "rewardResolver": {
            "type": "string",
            "description": "奖励对象"
          },
          "singleWire": {
            "type": "integer",
            "format": "int64",
            "description": "单注可押注最大金额"
          }
        }
      },
      "RouletteCounter": {
        "title": "RouletteCounter",
        "type": "object",
        "properties": {
          "propertySize": {
            "type": "integer",
            "description": "已领取数量"
          },
          "canReceiveTime": {
            "type": "integer",
            "description": "可领取时间戳"
          },
          "enterTheRoulette": {
            "type": "boolean",
            "description": "是否可进入轮盘"
          }
        }
      },
      "Prize": {
        "title": "Prize",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "奖名"
          },
          "prize": {
            "type": "integer",
            "description": "多少钱"
          },
          "type": {
            "type": "integer",
            "description": "类型"
          }
        }
      },
      "CollectResult": {
        "title": "CollectResult",
        "type": "object",
        "properties": {
          "rouletteCounter": {
            "$ref": "#/components/schemas/RouletteCounter"
          },
          "prize": {
            "type": "integer",
            "description": "领到了多少钱"
          }
        }
      },
      "PiggyBank": {
        "title": "PiggyBank",
        "type": "object",
        "properties": {
          "state": {
            "type": "integer",
            "description": "0未满 1已满"
          },
          "payState": {
            "type": "integer",
            "description": "0未支付 1已支付"
          },
          "money": {
            "type": "number",
            "description": "已存金额"
          }
        }
      },
      "SetBalance": {
        "title": "SetBalance",
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer"
          },
          "number": {
            "type": "integer"
          }
        },
        "required": [
          "userId",
          "number"
        ]
      },
      "UserInfo": {
        "title": "UserInfo",
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "description": "用户ID"
          },
          "nickName": {
            "type": "string",
            "description": "用户昵称"
          },
          "headPortrait": {
            "description": "用户头像",
            "type": "integer"
          }
        }
      },
      "Balance": {
        "title": "Balance",
        "type": "object",
        "properties": {
          "gold": {
            "type": "string",
            "description": "用户余额"
          }
        }
      },
      "Time": {
        "title": "Time",
        "type": "object",
        "properties": {
          "nowtime": {
            "type": "integer",
            "description": "当前时间戳"
          }
        },
        "required": [
          "nowtime"
        ]
      },
      "TreasureChest": {
        "title": "TreasureChest",
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "用户的宝箱ID"
          },
          "level": {
            "type": "string",
            "description": "宝箱的等级"
          },
          "time": {
            "type": "integer",
            "description": "开启需要的时间"
          },
          "countdown": {
            "type": "integer",
            "description": "倒计时"
          },
          "openState": {
            "type": "integer",
            "description": "0倒计时中(不可开启) 1可开启 2未开启倒计时"
          },
          "speedUp": {
            "type": "boolean",
            "description": "true可加速 false不可加速"
          }
        }
      },
      "TreasureChestId": {
        "title": "TreasureChestId",
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      },
      "TreasureChestPrize": {
        "title": "TreasureChestPrize",
        "type": "object",
        "properties": {
          "prize": {
            "type": "integer",
            "description": "奖品"
          }
        }
      },
      "AcceptOrPass": {
        "title": "AcceptOrPass",
        "type": "object",
        "properties": {
          "acceptOrPass": {
            "type": "boolean",
            "description": "true领取 false丢弃"
          }
        }
      }
    },
    "securitySchemes": {
      "user_token": {
        "type": "apiKey",
        "name": "user_token",
        "in": "query"
      },
      "admin_token": {
        "type": "apiKey",
        "name": "admin_token",
        "in": "query"
      }
    }
  }
